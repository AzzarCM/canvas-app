n        }\r\n        \r\n    }\r\n\r\n    const handleFacebookLogin = () => {\r\n        dispatch( startFacebookLogin() );\r\n    }\r\n\r\n    const handleGoogleLogin = () => {\r\n        dispatch( startGoogleLogin() );\r\n    }\r\n\r\n    return (\r\n        <div className=\"auth__container animate__animated animate__fadeIn\">\r\n            <div className=\"auth__image\">\r\n                <img alt=\"una imagen\" src={imagen} />\r\n            </div>\r\n            <div className=\"auth__right-container\">\r\n                <div className=\"auth__mini-nav\">\r\n                    <a href=\"/main/home\">\r\n                    <img alt=\"logo\" src={logo}/>\r\n                    </a>\r\n                    <div style={{\r\n                        display:'flex', \r\n                        alignItems:'center'}}>\r\n                        <p style={{paddingRight:20, marginBottom:0}}>Sin cuenta?</p>\r\n                        <a href=\"/auth/register\">\r\n                            <button className=\"btn-medium\">\r\n                                Registrate\r\n                            </button>\r\n                        </a>\r\n                        \r\n                    </div>  \r\n                </div>\r\n                <h2 className=\"auth__h2-align\">Iniciar sesión</h2>\r\n                {\r\n                msgError &&\r\n                (\r\n                    <div className=\"auth__alert-error\">\r\n                        {msgError}\r\n                    </div>\r\n                )\r\n                \r\n                }\r\n                <form className=\"auth__form\" onSubmit={ handleLogin }>\r\n                    <input \r\n                        type=\"text\"\r\n                        name=\"email\" \r\n                        placeholder=\"Correo\"\r\n                        className=\"auth__input\"\r\n                        value={ email }\r\n                        onChange={ handleInputChange }\r\n                    />\r\n                    <input \r\n                        type=\"password\" \r\n                        name=\"password\"\r\n                        placeholder=\"Contraseña\"\r\n                        className=\"auth__input\"\r\n                        value={ password }\r\n                        onChange={ handleInputChange }\r\n\r\n                    />\r\n                    <button\r\n                        type=\"submit\"\r\n                        className=\"btn btn-primary\"\r\n                        disabled={loading}\r\n                    >\r\n                        Iniciar sesion\r\n                    </button>\r\n                </form>\r\n                <button \r\n                    className=\"btn-facebook color-fb\"\r\n                    onClick={ handleFacebookLogin }\r\n                >\r\n                    <i style={{marginRight:30}} className=\"fab fa-facebook-f\"></i>\r\n                    Iniciar sesión con facebook\r\n                </button>\r\n                <div onClick={ handleGoogleLogin } className=\"auth__social-networks\">\r\n                    <div className=\"google-btn\">\r\n                        <div className=\"google-icon-wrapper\">\r\n                            <img className=\"google-icon\" src=\"https://upload.wikimedia.org/wikipedia/commons/5/53/Google_%22G%22_Logo.svg\" alt=\"google button\" />\r\n                        </div>\r\n                        <p className=\"btn-text\">\r\n                        <b>Sign in with google</b>\r\n                        </p>\r\n                    </div>\r\n                </div>\r\n            </div>\r\n        </div>\r\n    )\r\n}\r\n","C:\\React\\canvas-app\\src\\components\\Auth\\RegisterScreen.js",[],"C:\\React\\canvas-app\\src\\components\\main\\PrincipalScreen.js",["206"],"import React from 'react'\r\n\r\n//components\r\n\r\nimport { Footer } from './Footer';\r\nimport { Carousel } from './Carousel'\r\nimport { Navbar } from './Navbar'\r\n\r\n//assets\r\n