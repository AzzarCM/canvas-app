     })\r\n        setNombre(nombreTheme)\r\n        setImagenes(imagenes);\r\n        setBandera(false);\r\n    }\r\n\r\n    return (\r\n        <div style={{width: \"100%\"}}>\r\n            {\r\n                bandera ? \r\n                <h1>Loading</h1> :\r\n                <div>\r\n                    <h1 style={{\r\n                        textAlign: 'center',\r\n                    }}>\r\n                        {`Busqueda: \"${nombre}\"`}\r\n                    </h1>\r\n                    {\r\n                        imagenes[0] ? \r\n                        <div className=\"temas__wrap-container\">\r\n                        {imagenes[0].map(item =>{\r\n                            return (\r\n                                <ImageItem key={item.id} img={item}/>\r\n                                )\r\n                            })\r\n                        }\r\n                        </div> :\r\n\r\n                        <h1>No hay cuadros disponibles:(</h1>\r\n                    }   \r\n                </div> \r\n            }\r\n        </div>\r\n    )\r\n}\r\n","C:\\React\\canvas-app\\src\\components\\temas\\SideBar.js",[],"C:\\React\\canvas-app\\src\\store\\store.js",[],"C:\\React\\canvas-app\\src\\reducers\\authReducer.js",[],"C:\\React\\canvas-app\\src\\types\\types.js",[],"C:\\React\\canvas-app\\src\\hooks\\useForm.js",[],"C:\\React\\canvas-app\\src\\actions\\auth.js",[],"C:\\React\\canvas-app\\src\\firebase\\firebase-config.js",[],"C:\\React\\canvas-app\\src\\reducers\\uiReducer.js",[],"C:\\React\\canvas-app\\src\\actions\\ui.js",[],"C:\\React\\canvas-app\\src\\routers\\PrivateRoute.js",[],"C:\\React\\canvas-app\\src\\routers\\PublicRoute.js",[],"C:\\React\\canvas-app\\src\\reducers\\cartReducer.js",["157","158","159","160","161","162","163","164","165","166","167","168","169","170","171"],"\r\nimport { addQuantity } from \"../actions/cart\"\r\nimport {types} from \"../types/types\"\r\n\r\nconst initialState = {\r\n    items: [],\r\n    addedItems: [],\r\n    total: 0\r\n}\r\n   \r\n\r\n\r\nexport const cartReducer = (state = initialState, action) =>{\r\n    switch (action.type) {\r\n        case types.FILL_ITEMS:\r\n            return {\r\n                ...state,\r\n                items: action.payload\r\n            }\r\n        case types.ADD_TO_CART:\r\n            var addedItem = state.items.find(item=>item.id == action.id)\r\n            \r\n            var existed_item= state.addedItems.find(item=> action.id == item.id)\r\n        \r\n            if(existed_item)\r\n            {\r\n            addedItem.quantity += 1 \r\n             return{\r\n                ...state,\r\n                 total: state.total + parseFloat(addedItem.price)\r\n                  }\r\n            }\r\n         else{\r\n            addedItem.quantity = 1;\r\n            //calculating the total\r\n            var newTotal = state.total + parseFloat(addedItem.price)\r\n            return{\r\n                ...state,\r\n                addedItems: [...state.addedItems, addedItem],\r\n                total : newTotal\r\n            }\r\n            \r\n        }\r\n        \r\n        case types.ADD_QUANTITY:\r\n            var addedItem = state.addedItems.find(item=> item.id == action.id);\r\n            addedItem.quantity += 1\r\n            console.log(addedItem);\r\n            var newTotal = state.total + parseFloat(addedItem.price)\r\n            return{\r\n                ...state,\r\n                total: newTotal\r\n            }\r\n        case types.SUB_QUANTITY:\r\n            var addedItem = state.addedItems.find(item=> item.id == action.id);\r\n            //if the qt == 0 then it should be removed\r\n            if(addedItem.quantity === 1){\r\n                var new_items = state.addedItems.filter(item=>item.id != action.id)\r\n                var newTotal = state.total - addedItem.price\r\n                return{\r\n                    ...state,\r\n                    addedItems: new_items,\r\n                    total: newTotal\r\n                }\r\n            }\r\n            else {\r\n                addedItem.quantity -= 1\r\n                var newTotal = state.total - addedItem.price\r\n                return{\r\n                    ...state,\r\n                    total: newTotal\r\n                }\r\n            }\r\n        case types.REMOVE_ITEM:\r\n            let itemToRemove= state.addedItems.find(item=> action.id == item.id)\r\n            var new_items = state.addedItems.filter(item=> action.id != item.id)\r\n            \r\n            //calculating the total\r\n            var newTotal = state.total - (itemToRemove.price * itemToRemove.quantity )\r\n            return{\r\n                ...state,\r\n                addedItems: new_items,\r\n                total: newTotal\r\n            }\r\n        default:\r\n            return state\r\n    }\r\n}","C:\\React\\canvas-app\\src\\actions\\cart.js",[],"C:\\React\\canvas-app\\src\\helpers\\loadPaintings.js",[],"C:\\React\\canvas-app\\src\\components\\cart\\Cart.js",[],{"ruleId":"172","replacedBy":"173"},{"ruleId":"174","replacedBy":"175"},{"ruleId":"176","severity":1,"message":"177","line":24,"column":8,"nodeType":"178","endLine":24,"endColumn":10,"suggestions":"179"},{"ruleId":"176","severity":1,"message":"180","line":23,"column":8,"nodeType":"178","endLine":23,"endColumn":10,"suggestions":"181"},{"ruleId":"172","replacedBy":"182"},{"ruleId":"174","replacedBy":"183"},{"ruleId":"176","severity":1,"message":"184","line":12,"column":8,"nodeType":"178","endLine":12,"endColumn":10,"suggestions":"185"},{"ruleId":"186","severity":1,"message":"187","line":2,"column":10,"nodeType":"188","messageId":"189","endLine":2,"endColumn":21},{"ruleId":"190","severity":1,"message":"191","line":21,"column":60,"nodeType":"192","messageId":"193","endLine":21,"endColumn":62},{"ruleId":"190","severity":1,"message":"191","line":23,"column":70,"nodeType":"192","messageId":"193","endLine":23,"endColumn":72},{"ruleI