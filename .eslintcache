s = state.addedItems.filter(item=> action.id != item.id)\r\n            \r\n            //calculating the total\r\n            var newTotal = state.total - (itemToRemove.price * itemToRemove.quantity )\r\n            return{\r\n                ...state,\r\n                addedItems: new_items,\r\n                total: newTotal\r\n            }\r\n        case types.LOG_OUT:\r\n            return {\r\n                ...state,\r\n                addedItems: [],\r\n                total: 0\r\n            }\r\n        case types.CHANGE_TOTAL:\r\n            return {\r\n                ...state,\r\n                total: action.payload\r\n            }\r\n        default:\r\n            return state\r\n    }\r\n}","C:\\React\\canvas-app\\src\\actions\\cart.js",[],"C:\\React\\canvas-app\\src\\helpers\\loadPaintings.js",[],"C:\\React\\canvas-app\\src\\components\\cart\\Cart.js",["228"],"C:\\React\\canvas-app\\src\\components\\cart\\Checkout.js",["229","230","231","232","233"],"C:\\React\\canvas-app\\src\\components\\cart\\CartList.js",["234","235"],"C:\\React\\canvas-app\\src\\components\\history\\Historial.js",["236"],"C:\\React\\canvas-app\\src\\components\\search\\Search.js",["237","238","239"],"C:\\React\\canvas-app\\src\\components\\search\\SearchBar.js",[],"C:\\React\\canvas-app\\src\\components\\information\\TermsAndCond.js",[],"C:\\React\\canvas-app\\src\\components\\information\\Garantia.js",[],{"ruleId":"240","replacedBy":"241"},{"ruleId":"242","replacedBy":"243"},{"ruleId":"244","severity":1,"message":"245","line":13,"column":10,"nodeType":"246","messageId":"247","endLine":13,"endColumn":22},{"ruleId":"244","severity":1,"message":"248","line":14,"column":10,"nodeType":"246","messageId":"247","endLine":14,"endColumn":21},{"ruleId":"249","severity":1,"message":"250","line":34,"column":8,"nodeType":"251","endLine":34,"endColumn":10,"suggestions":"252"},{"ruleId":"244","severity":1,"message":"253","line":14,"column":11,"nodeType":"246","messageId":"247","endLine"