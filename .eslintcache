t resp.json();\r\n        const imagenes = paintings.map( img =>{\r\n            return {\r\n                id: img.id,\r\n                name: img.name,\r\n                description: img.description,\r\n                stock: img.stock,\r\n                active: img.active,\r\n                measurements: img.measurements,\r\n                price: img.price,\r\n                image_url: img.image_url,\r\n                materials: img.materials,\r\n            }\r\n        })\r\n        dispatch( fillItems(imagenes));\r\n     }\r\n\r\n    if(checking){\r\n        return(\r\n            <section style={{\r\n                display: 'flex',\r\n                justifyContent: 'center',\r\n                height: '100vh',\r\n                width: '100vw',\r\n            }} {...containerProps}>\r\n                {indicatorEl}\r\n            </section>\r\n        )\r\n    }\r\n\r\n    return (\r\n       <Router>\r\n           <div>\r\n               <Switch>\r\n                   <Route\r\n                        path=\"/auth\"\r\n                        component={ (props) => (\r\n                            (isLoggedIn) \r\n                            ? (<Redirect to=\"/\"/>)\r\n                            : (<AuthRouter/>)\r\n                        )}\r\n                   />\r\n                   <Route\r\n                        path=\"/\"\r\n                        component={ PrincipalRouter }\r\n                   />\r\n\r\n                   <Redirect to=\"/\"/>\r\n               </Switch>\r\n\r\n           </div>\r\n       </Router>\r\n    )\r\n}\r\n","D:\\Cristian JR\\canvas-app\\src\\store\\store.js",[],"D:\\Cristian JR\\canvas-app\\src\\routers\\PrincipalRouter.js",[],"D:\\Cristian JR\\canvas-app\\src\\routers\\AuthRouter.js",[],"D:\\Cristian JR\\canvas-app\\src\\actions\\cart.js",[],"D:\\Cristian JR\\canvas-app\\src\\constants\\URLS.js",[],"D:\\Cristian JR\\canvas-app\\src\\actions\\auth.js",[],"D:\\Cristian JR\\canvas-app\\src\\reducers\\uiReduc